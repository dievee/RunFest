@model List<User>
@{
    ViewData["Title"] = "Finish";
}
@if (Model != null && Model.Count != 0)
{
    @foreach (var user in Model)
    {
        <div style="background-color: gainsboro;">
            <p>@user.FullName  @user.RunningNumber</p>
            <br />
            <div>
                <p style="color: green;">@user.StartTime</p>
                <br />
                <p>@user.FinishTime</p>
            </div>
            <div>
                <form method="post" action="~/Run/Finish">
                    <input type="hidden" name="userId" value="@user.Id" /><br />
                    <input type="submit" value="Finish" class="btn btn-danger" />
                </form>
            </div>

        </div>
    }
}
else
{
    <p>There is not data in database</p>
}