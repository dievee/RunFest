@model List<User>
@{
    ViewData["Title"] = "Finish";
}
@if (Model != null && Model.Count != 0)
{
    <table class="table">
        <tr><th>Full Name</th><th>Running Number</th><th>Start Time</th><th></th></tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FullName</td>
                <td>@user.RunningNumber</td>
                <td>@user.StartTime.TimeOfDay.ToString(@"hh\:mm\:ss")</td>
                <td>
                    <form method="post" action="~/Run/Finish">
                        <input type="hidden" name="userId" value="@user.Id" />
                        <input type="submit" value="Finish" class="btn btn-danger" />
                    </form>
                </td>
            </tr>
        }
    </table>
}
else
{
    <p>There is not data in database</p>
}