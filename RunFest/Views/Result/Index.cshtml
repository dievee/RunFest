@model List<User>
@{
    ViewData["Title"] = "Result";
}
@if (Model != null && Model.Count != 0)
{
    <table class="table">
        <tr><th>Full Name</th><th>Running Number</th><th>Result</th></tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.FullName</td>
                <td>@user.RunningNumber</td>
                <td>@user.ResultTime.ToString(@"hh\:mm\:ss")</td>
            </tr>
        }
    </table>
}
else
{
    <p>There is not data in database</p>
}